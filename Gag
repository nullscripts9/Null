-- Function to get emoji
local function getEmoji(name, itemType)
    local lower = name:lower()
    if itemType == "Pet" then
        if name == "Red Fox" then return "ðŸ¦Š" end
        if name == "Raccoon" then return "ðŸ¦" end
        if name == "Dragonfly" then return "ðŸ¦Ÿ" end
        if name == "Monkey" then return "ðŸ’" end
        if name == "Orange Tabby" then return "ðŸˆ" end
        if name == "Pig" then return "ðŸ–" end
        if name == "Rooster" then return "ðŸ“" end
        if name == "Firefly" then return "ðŸª°" end
        return "ðŸ¾"
    else
        if lower:find("coconut") then return "ðŸ¥¥" end
        if lower:find("tomato") then return "ðŸ…" end
        if lower:find("daffodil") then return "ðŸŒ¼" end
